<%= simple_form_for @flat do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price_per_night %>
  <%= f.input :number_of_guests %>
  <%= f.input :number_of_bedrooms %>
  <%= f.input :number_of_bathrooms %>
  <%= f.input :picture_url %>
  <%= f.submit %>
<% end %>
